generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Referrals{
  id    String    @id @default(uuid())
  referred_by   String
  referred_by_name    String
  referred_to   String
}
